<comment> comment = ';' [^\n]* '\n';

file = object+;

object = name ':' ( ( value ( ',' value )* )? block | value ( ',' value )* );

<token> name = [\l_] [\l\d_]*;

value = ( number | string | array );

block = '{' object* '}';

<token> number = [+-]? [\d]+ ( '.' [\d]+ )?;

<token> string = '"' [^"]* '"';

array = '*' number block;

